# Default make file
PKG_CPPFLAGS  += -I. `pkg-config --cflags protobuf grpc` -1thread
PKG_LIBS += -I. `pkg-config --libs protobuf grpc` -1thread

# Include all C++ files in src/ and its subdirectories
SUBDIR_SOURCES = @SUBDIR_SOURCES@
SOURCES=@SUBDIR_SOURCES@ ./RcppExports.cpp ./bqs.cpp

# Obtain the object files
OBJECTS=$(SUBDIR_SOURCES:.cc=.o) RcppExports.o bqs.o

# Make the shared object
all: $(SHLIB)

# Provide recipe to remove all objects
clean: echo Non
#@rm -f $(OBJECTS)
