# Default make file
PKG_CPPFLAGS += `pkg-config --cflags protobuf grpc`
PKG_CXXFLAGS += -std=c++11 -I.
PKG_LIBS += -L/usr/local/lib -L/usr/lib -L/usr/share/lib `pkg-config --libs protobuf grpc++`\
            -pthread\
            -Wl,--no-as-needed -lgrpc++_reflection -Wl,--as-needed\
            -ldl\
            -I.

# Obtain the object files
OBJECTS=google/api/resource.pb.o google/api/field_behavior.pb.o google/api/client.pb.o google/api/http.pb.o google/api/annotations.pb.o google/cloud/bigquery/storage/v1/arrow.pb.o google/cloud/bigquery/storage/v1/avro.pb.o google/cloud/bigquery/storage/v1/stream.pb.o google/cloud/bigquery/storage/v1/storage.pb.o google/api/resource.grpc.pb.o google/api/field_behavior.grpc.pb.o google/api/client.grpc.pb.o google/api/http.grpc.pb.o google/api/annotations.grpc.pb.o google/cloud/bigquery/storage/v1/arrow.grpc.pb.o google/cloud/bigquery/storage/v1/avro.grpc.pb.o google/cloud/bigquery/storage/v1/stream.grpc.pb.o google/cloud/bigquery/storage/v1/storage.grpc.pb.o cpp11.o bqs.o
